<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net">
  <object id="Ingredients" type="List&lt;IIngredient>">
    <constructor-arg>
      <list element-type="IIngredient">
        <ref object="Shrimp" />
        <ref object="Vinaigrette" />
        <ref object="Mayonnaise" />
      </list>
    </constructor-arg>
  </object>
  <object id="Shrimp" type="Shrimp" singleton="true" />
  <object id="OliveOil" type="OliveOil" singleton="true" />
  <object id="EggYolk" type="EggYolk" singleton="true" />
  <object id="Vinegar" type="Vinegar" singleton="true" />
  <object id="Vinaigrette" type="Vinaigrette" autowire="autodetect" singleton="true" />
  <object id="Mayonnaise" type="Mayonnaise" autowire="autodetect" singleton="true" />
  <object id="Course" type="Course" autowire="autodetect" singleton="true" />
</objects>